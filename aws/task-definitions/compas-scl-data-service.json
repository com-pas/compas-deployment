{
    "containerDefinitions": [
        {
            "name": "CoMPAS-Scl-Data-Service",
            "image": "lfenergy/compas-scl-data-service:0.13.0-postgresql",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [
                {
                    "name": "MP_JWT_TOKEN_HEADER",
                    "value": "x-amzn-oidc-accesstoken"
                },
                {
                    "name": "POSTGRESQL_USERNAME",
                    "value": ""
                },
                {
                    "name": "JWT_VERIFY_KEY",
                    "value": ""
                },
                {
                    "name": "POSTGRESQL_PASSWORD",
                    "value": ""
                },
                {
                    "name": "POSTGRESQL_HOST",
                    "value": ""
                },
                {
                    "name": "QUARKUS_HTTP_ACCESS_LOG_PATTERN",
                    "value": "long"
                },
                {
                    "name": "USERINFO_SESSION_EXPIRES",
                    "value": "30"
                },
                {
                    "name": "JWT_VERIFY_ISSUER",
                    "value": ""
                },
                {
                    "name": "POSTGRESQL_DB",
                    "value": ""
                },
                {
                    "name": "USERINFO_WHO_CLAIMNAME",
                    "value": "username"
                },
                {
                    "name": "USERINFO_SESSION_WARNING",
                    "value": "20"
                },
                {
                    "name": "JWT_VERIFY_CLIENT_ID",
                    "value": ""
                },
                {
                    "name": "QUARKUS_HTTP_ACCESS_LOG_ENABLED",
                    "value": "true"
                },
                {
                    "name": "QUARKUS_LOG_LEVEL",
                    "value": "ALL"
                },
                {
                    "name": "JWT_GROUPS_PATH",
                    "value": "cognito:groups"
                },
                {
                    "name": "USERINFO_NAME_CLAIMNAME",
                    "value": "username"
                },
                {
                    "name": "SCL_DATA_SERVICE_SOFT_DELETE_ENABLED",
                    "value": "true"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "dependsOn": [
                {
                    "containerName": "Postgresql",
                    "condition": "HEALTHY"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/scl-data-service",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "Postgresql",
            "image": "postgres:14.5",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 5432,
                    "hostPort": 5432,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": ""
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": ""
                },
                {
                    "name": "POSTGRES_DB",
                    "value": ""
                },
                {
                    "name": "PGUSER",
                    "value": ""
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/scl-data-service",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "\"pg_isready\"",
                    "\"-d\"",
                    "\"compas\""
                ],
                "interval": 30,
                "timeout": 60,
                "retries": 5,
                "startPeriod": 80
            }
        }
    ],
    "family": "CoMPAS-Scl-Data-Service",
    "executionRoleArn": "ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
